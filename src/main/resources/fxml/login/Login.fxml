<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.PasswordField?>
<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="201.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.login.LoginController">
   <children>
      <VBox fx:id="vBoxTwo" alignment="CENTER" fillWidth="false" prefHeight="195.0" prefWidth="200.0">
         <children>
            <HBox alignment="BOTTOM_LEFT" fillHeight="false" prefHeight="100.0" prefWidth="279.0">
               <children>
                  <TextField fx:id="errorBox" alignment="TOP_LEFT" prefHeight="20.0" prefWidth="199.0" style="-fx-control-inner-background: red;" text="Invalid Username and/or password" />
               </children>
            </HBox>
            <TextField fx:id="usernameTextField" prefHeight="25.0" prefWidth="280.0" promptText="username" />
            <PasswordField fx:id="passwordTextField" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="280.0" promptText="password" VBox.vgrow="ALWAYS" />
            <Button fx:id="signInButton" mnemonicParsing="false" text="Sign in" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
